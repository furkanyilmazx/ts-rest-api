{
  "name": "ts-rest-api",
  "version": "1.0.0",
  "private": true,
  "description": "typescript rest api boilerplate",
  "main": "src/index.ts",
  "scripts": {
    "build": "webpack --mode production --env API_ENV=local  --env DEBUG_MODE=disabled --env BUILD_DIR=./build",
    "build:stg": "webpack --mode production --env API_ENV=stg  --env DEBUG_MODE=disabled  --env BUILD_DIR=./build",
    "build:sandbox": "webpack --mode production --env API_ENV=sandbox  --env DEBUG_MODE=disabled  --env BUILD_DIR=./build",
    "build:prod": "webpack --mode production --env API_ENV=prod  --env DEBUG_MODE=disabled  --env BUILD_DIR=./build",
    "build:disaster": "webpack --mode production --env API_ENV=disaster  --env DEBUG_MODE=disabled  --env BUILD_DIR=./build",
    "develop": "webpack --mode development --watch --env API_ENV=local  --env DEBUG_MODE=enabled  --env BUILD_DIR=./build",
    "start": "yarn build && node build/index.local.bundle.js",
    "test": "jest --watchAll=false",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "heroku-prebuild": "yarn install --production=false"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "prettier --write",
      "yarn lint"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "engines": {
    "node": "14.x"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "rest-api"
  ],
  "author": "furkanyilmazx",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.10",
    "@types/express": "4.17.9",
    "@types/jest": "^26.0.19",
    "@types/node": "14.14.12",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-dynamic-import-node-babel-7": "2.0.7",
    "clean-webpack-plugin": "3.0.0",
    "eslint": "^7.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-webpack-plugin": "^2.4.1",
    "filemanager-webpack-plugin": "2.0.5",
    "husky": "^4.3.5",
    "jest": "26.6.3",
    "lint-staged": "^10.5.3",
    "nodemon-webpack-plugin": "4.3.2",
    "prettier": "^2.2.1",
    "terser-webpack-plugin": "5.0.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "8.0.12",
    "typescript": "4.1.2",
    "webpack": "5.10.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "axios": "^0.21.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.1.4",
    "i18n": "^0.13.2",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.20",
    "util": "^0.12.3",
    "uuid": "^8.3.2",
    "validator": "^13.5.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  }
}
